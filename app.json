    ],
    "runtimeVersion": "1.0.0",
    "extra": {
      "eas": {
        "projectId": "34582600-811f-4183-b0a0-861c46c64826",
        "appleTeamId": "D9446CQ8S2",
        "appleClientId": "dev.millionsolutions.faduapp",
